{
  "name": "@zbox/browser",
  "version": "0.3.0",
  "description": "Browser javascript binding for ZboxFS",
  "keywords": [
    "webassembly",
    "wasm",
    "filesystem",
    "crypto",
    "security",
    "privacy",
    "rust"
  ],
  "homepage": "https://github.com/zboxfs/zbox-browser",
  "license": "Apache-2.0",
  "author": "Zbox.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/zboxfs/zbox-browser"
  },
  "scripts": {
